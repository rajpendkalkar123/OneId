# Environment variables for blockchain deployment
# Add your private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Holesky testnet RPC URL (optional - defaults to public node)
HOLESKY_RPC_URL=https://ethereum-holesky.publicnode.com

# Etherscan API key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Frontend contract addresses per network
# Will be set after deployment
REACT_APP_AADHAR_DID_ADDRESS_HOLESKY=
REACT_APP_AADHAR_DID_ADDRESS_LOCALHOST=0x5FbDB2315678afecb367f032d93F642f64180aa3
